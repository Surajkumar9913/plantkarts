<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>signupservlet</servlet-name>
        <servlet-class>plantkartservlet.signupservlet</servlet-class>
        <init-param>
            <param-name>Name</param-name>
            <param-value>Value</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>plantkartservlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PlantOrderServlet</servlet-name>
        <servlet-class>plantkartservlet.PlantOrderServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteKartItemServlet</servlet-name>
        <servlet-class>plantkartservlet.DeleteKartItemServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PlantPaymentSuccessServlet</servlet-name>
        <servlet-class>plantkartservlet.PlantPaymentSuccessServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OrderDeleteServlet</servlet-name>
        <servlet-class>plantkartservlet.OrderDeleteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>signupservlet</servlet-name>
        <url-pattern>/signupservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PlantOrderServlet</servlet-name>
        <url-pattern>/PlantOrderServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteKartItemServlet</servlet-name>
        <url-pattern>/DeleteKartItemServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PlantPaymentSuccessServlet</servlet-name>
        <url-pattern>/PlantPaymentSuccessServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OrderDeleteServlet</servlet-name>
        <url-pattern>/OrderDeleteServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
